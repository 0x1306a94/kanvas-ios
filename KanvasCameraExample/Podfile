    pod 'Utils', :path => '../../Utils'
    pod 'ImageLoader', :path => '../../ImageLoader'
    pod 'CoreTumblr', :path => '../../CoreTumblr'
    pod 'DataDisplayKit', :path => '../../DataDisplayKit'
    pod 'NetworkAbstractions', :path => '../../NetworkAbstractions'
    pod 'TMTimelineObject', :path => '../../TMTimelineObject'
    pod 'PostForms', :path => '../../PostForms'
    pod 'Timelines', :path => '../../Timelines'
end

target 'KanvasCameraExampleTests' do
    pod 'KanvasCamera', :path => '../'
    pod 'FBSnapshotTestCase', '2.1.4'
end
